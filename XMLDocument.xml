    <profil id="1">
        <zakladni>
            <jazyk>Čeština</jazyk>
            <jmeno_profilu>Charaktery</jmeno_profilu>
            <menu vlastnost="Soubor extra funkcí">Rozšířené</menu>
            <tvurce pohlavi="muž" vek= "63 let">Petr Skočdopole</tvurce>
            <ukladani_projektu ulozeni="C:\Zbrush\characters">Složka charekterů Zbrush</ukladani_projektu>
            <vytvoreni_profilu>
                <datum>2015-11-05</datum>
                <cas>11:22:55</cas>
            </vytvoreni_profilu>
        </zakladni>
        <nastaveni_modelu>
            <tvar>
                <vychozi_tvar>Human</vychozi_tvar>
                <polygonalni_rozliseni polygony="50000">Vysoké</polygonalni_rozliseni>
                <tvar_polygonu>Čtverce</tvar_polygonu>
                <velikost_modelu>50x40x30</velikost_modelu>
                <umisteni>0,0,0</umisteni>
                <rotace>0,0,0</rotace>
            </tvar>
            <osy>
                <umisteni>20,20,20</umisteni>
                <rotace>0,0,0</rotace>
                <viditelnost>false</viditelnost>
            </osy>
            <textura>
                <barva>Červená</barva>
                <odrazivost>20</odrazivost>
                <hrbolatost>0</hrbolatost>
                <jas>10</jas>
                <sum>30</sum>
                <alpha>35</alpha>
            </textura>
        </nastaveni_modelu>
        <scena>
            <pozadi typ=".jpg">Obloha</pozadi>
            <slunce>Přednastavené</slunce>
            <obloha>Přednastavené</obloha>
            <stin>Ostrý</stin>
            <mraky>Volumetrické</mraky>
            <podlaha>Přednastavené</podlaha>  
        </scena>
        <render>
            <rendrovaci_system>Standardní</rendrovaci_system>
            <vystup>
                <sirka>1280</sirka>
                <vyska>720</vyska>
                <rozliseni>72 pixelů</rozliseni>
                <soubor typ=".png">render</soubor>
            </vystup>
            <vyhlazovani>
                <vyhlazeni>Geometrie</vyhlazeni>
                <filtr>Kubický</filtr>
                <vlastni_velikost>true</vlastni_velikost>
                <mira_mip mip="50">Vysoká</mira_mip>
                <male_fragmenty>Hybridní</male_fragmenty>
            </vyhlazovani>
            <globalni_iluminace>
                <primarni_metoda>Irradiance Cache</primarni_metoda>
                <sekundarni_metoda>QMC</sekundarni_metoda>
                <mnozstvi_odrazu pocet_odrazu="5">Vysoká</mnozstvi_odrazu>
                <gammut>1</gammut>
            </globalni_iluminace>
            <ambient_occlusion>
                <barva>Černá</barva>
                <rozptyl>90</rozptyl>
                <presnost>50</presnost>
                <kontrast>0</kontrast>
                <pouzit>true</pouzit>
            </ambient_occlusion>
        </render>
    </profil>
         
